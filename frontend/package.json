        "@storybook/addon-essentials": "8.6.14",
        "@storybook/react-vite": "8.6.14",
        "@types/react": "^19.2.14",
        "@types/react-dom": "^19.2.3",
        "copyfiles": "2.4.1",
        "eslint": "^10.0.1",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-react-hooks": "^7.0.1",
        "react": "^19.2.4",
        "react-dom": "^19.2.4",
